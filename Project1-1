#include <iostream>
#include <stdio.h>
#include <stdlib.h>

using namespace std;

int prime(int number){
	int m;
	int flag=0;
    m=number/2;
    for(int i=2; i<=m; i++)
    {
    	if(number%i==0)
		{
        	flag=1;
        	break;
      	}
	}
  	return flag;
}

int grnp(int range0, int range1){
	int p=rand();
	int ispprime=prime(p);
	while (p<range0 || p>range1 || ispprime==1){
		p=rand();
		ispprime=prime(p);
	}
	return p;
} 

int grnq(int range0, int range1){
	int q=rand();
	int isqprime=prime(q);
	while (q<range0 || q>range1 || isqprime==1){
		q=rand();
		isqprime=prime(q);
	}
	return q;
}
