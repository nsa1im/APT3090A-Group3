#include <iostream>
#include <stdio.h>
#include <stdlib.h>

using namespace std;

int checkprime(int number){ //c++ programming language
	int m;
	int flag=0;
    	m=number/2;
    	for(int i=2; i<=m; i++)
    	{
    		if(number%i==0)
		{
        		flag=1;
        		break;
      		}
	}
  	return flag;
}

int p(int range0, int range1){ //c++ programming language
	int p=rand();
	int ispprime=checkprime(p);
	while (p<range0 || p>range1 || ispprime==1){
		p=rand();
		ispprime=checkprime(p);
	}
	return p;
} 

int q(int range0, int range1){ //c++ programming language
	int q=rand();
	int isqprime=checkprime(q);
	while (q<range0 || q>range1 || isqprime==1){
		q=rand();
		isqprime=checkprime(q);
	}
	return q;
}

int findgcd(int a, int b) { //c++ programming language
   	if (b == 0){
   		return a;
   	}
   	return findgcd(b, a % b);
}

//generating n and phi(euler totient function)

int n(int p, int q) //c++ programming language
{
n = p * q;
return n;
}
    
int getTotient(int p, int q) //c++ programming language
{
int phi = (p - 1) * (q - 1);
return phi;
}







//java programming language
public static void main(){  
try{  
ServerSocket s1=new ServerSocket(6666);  
Socket s=s1.accept();                   //establishes connection   
DataInputStream input=new DataInputStream(s.getInputStream());  
String  x=(String)input.readUTF();  
System.out.println("message= "+ x);  
ss.close();  
}catch(Exception e){System.out.println(e);}  
}  
} 


